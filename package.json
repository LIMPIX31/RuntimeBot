{
  "name": "runtime-bot",
  "version": "1.0.0",
  "description": "Discord bot for running simple codeblock(s) in discord",
  "main": "src/index.ts",
  "repository": "https://github.com/LIMPIX31/RuntimeBot.git",
  "author": "LIMPIX31",
  "license": "gpl-3.0",
  "private": false,
  "scripts": {
