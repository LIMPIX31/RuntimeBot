    "dotenv": "^10.0.0",
    "inversify": "^6.0.1",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.7",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0"
  }
